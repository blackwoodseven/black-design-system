{
  "name": "black-design-system",
  "version": "0.2.3",
  "private": false,
  "dependencies": {},
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^16.2.0",
    "color": "^2.0.1",
    "react-jss": "^8.2.0"
  },
  "devDependencies": {
    "react": "^16.2.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "react-scripts": "1.0.17",
    "@storybook/addon-actions": "^3.2.19",
    "@storybook/addon-links": "^3.2.19",
    "@storybook/react": "^3.2.19"
  },
  "scripts": {
    "build": "yarn build:es2015 && yarn build:es2015modules",
    "build:es2015": "NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "build:es2015modules": "NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "storybook": "NODE_PATH=./src start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "files": ["build/*"],
  "misc": {}
}
