{
  "name": "black-design-system",
  "version": "0.2.8",
  "private": false,
  "dependencies": {},
  "main": "build/index.js",
  "peerDependencies": {
    "color": "^2.0.1",
    "react": "^15.2.0",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.6",
    "@storybook/addon-links": "^3.3.6",
    "@storybook/addon-storyshots": "^3.3.6",
    "@storybook/react": "^3.3.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "color": "^2.0.1",
    "react": "^15.2.0",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "build": "yarn build:es2015 && yarn build:es2015modules",
    "build:es2015": "NODE_ENV=production babel ./src --ignore *.spec.js,stories.js --out-dir ./build",
    "build:es2015modules": "NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublish": "yarn run build"
  },
  "files": ["build/*"],
  "misc": {}
}
